    <id name="IdBilet">
      <column name="idBilet" sql-type="int" not-null="true" />
    </id>
    <property name="IdSpectacol">
      <column name="idSpectacol" sql-type="int" not-null="true" />
    </property>
    <property name="Rand">
      <column name="rand" sql-type="int" not-null="true" />
    </property>
    <property name="Coloana">
      <column name="coloana" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>