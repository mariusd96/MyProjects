<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Opera" namespace="Opera.model">
  <class name="Spectacol" table="spectacol">
    <id name="IdSpectacol">
      <column name="idSpectacol" sql-type="int" not-null="true" />
      <generator class="increment"/>
    </id>
    <property name="Gen">
      <column name="gen" sql-type="varchar(32)" not-null="true" />
    </property>
    <property name="Titlu">
      <column name="titlu" sql-type="varchar(128)" not-null="true" />
    </property>
    <property name="Regia">
      <column name="regia" sql-type="varchar(128)" not-null="true" />
    </property>
    <property name="DataPremiere">
      <column name="data_premiere" sql-type="datetime" not-null="true" />
    </property>
    <property name="NumarBilete">
      <column name="numar_bilete" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>